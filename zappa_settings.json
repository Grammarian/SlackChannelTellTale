            "*_test.py",
            ".env",
            ".git",
            ".gitignore",
            ".idea",
            ".python-version",
            ".vscode",
            "example-structures.txt",
            "images",
            "LICENSE",
            "README.md",
            "test-messages",
            "venv",
            "zappa_settings.json"
